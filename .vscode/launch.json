{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
	 "Launch React App in Chrome"
     	 "skipFiles": [
       		 "<node_internals>/**"
      ],
      "program": "${.github/workspace}/app.js",  
      "restart": true, 
      "console": "integratedTerminal",  
      "env": {  
        "NODE_ENV": "development"
      }
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch React App in Chrome",
      "url": "http://localhost:3000",  
      "webRoot": "${.github/workspace}/src",  
      "sourceMaps": true,  
      "preLaunchTask": "npm: start" 
    }
  ]
}

